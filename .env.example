# N8N Template MCP Server Configuration

# N8N API Configuration
N8N_API_BASE=https://api.n8n.io
ENABLE_GRAPHQL=false
N8N_GQL_URL=https://workflow-api.n8n.io/graphql

# Cache Configuration (in seconds)
CACHE_SEARCH_TTL=900        # 15 minutes for search results
CACHE_TEMPLATE_TTL=3600     # 1 hour for template details
CACHE_CATEGORIES_TTL=86400  # 24 hours for categories
CACHE_MAX_SIZE=1000         # Maximum number of cached items

# Rate Limiting
RATE_LIMIT_MAX=10           # Maximum requests per window
RATE_LIMIT_WINDOW=60000     # Rate limit window in milliseconds (1 minute)

# Development Settings
NODE_ENV=production
LOG_LEVEL=info